{
    "overrides": [{
            "files": "*.js",
            "options": {
                "arrowParens": "always",
                "singleQuote": true,
                "trailingComma": "all",
                "bracketSpacing": false,
                "endOfLine": "lf"
            }
        },
        {
            "files": "*.html",
            "options": {
                "tabWidth": 4,
                "endOfLine": "lf"
            }
        }
    ]
}